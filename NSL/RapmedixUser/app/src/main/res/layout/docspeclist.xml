<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/_6sdp"
    android:background="@drawable/shadow3"
    android:orientation="horizontal"
    android:padding="@dimen/_4sdp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_2sdp"
        android:layout_marginLeft="@dimen/_2sdp"
        android:layout_marginRight="@dimen/_2sdp"
        android:layout_marginTop="@dimen/_2sdp"
        android:gravity="center|left"
        android:padding="@dimen/_5sdp"
        android:weightSum="1">

        <ImageView
            android:id="@+id/dsl_icon"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginLeft="@dimen/_8sdp"
            android:scaleType="fitXY"
            android:src="@drawable/doctor_black" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="-15dp"
            android:layout_marginRight="-20dp"
            android:rotation="90"
            android:scaleType="fitXY"
            android:src="@drawable/dotted_line_red" />

        <com.example.pegasys.rapmedixuser.activity.customviews.CustomTextViewNormal
            android:id="@+id/tv_docspectitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:text="test"
            android:textColor="@color/black"
            android:textSize="11.5sp" />
    </LinearLayout>

</android.support.v7.widget.CardView><!--


</LinearLayout>-->
